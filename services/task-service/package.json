{
  "name": "@grpc/task-service",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "bun --watch ./app/index.ts --hot",
    "dev:server": "deno run --allow-net --allow-env --allow-read --allow-sys --watch ./app/server.ts",
    "dev:client": "deno run --allow-net --allow-env --allow-read --allow-sys --watch ./app/client.ts",
    "proto": "deno run --allow-read https://deno.land/x/grpc_basic@0.4.7/gen/dts.ts ./proto/task.proto > ./types/task.d.ts"
  },
  "dependencies": {
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.1",
    "uuid": "^10.0.0",
    "express": "^4.18.3"
  },
  "devDependencies": { "@types/express": "^5.0.0", "typescript": "^5.6.3" },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": ["ES2020", "DOM"]
  }
}
